(define (maximum-strong-pair-xor nums)
  (define n (length nums))
  (define max-xor 0)
  (for ([i (in-range n)])
    (for ([j (in-range n)])
      (when (<= (abs (- (list-ref nums i) (list-ref nums j))) (min (list-ref nums i) (list-ref nums j)))
        (define current-xor (bitwise-xor (list-ref nums i) (list-ref nums j)))
        (when (> current-xor max-xor)
          (set! max-xor current-xor))))
  max-xor)