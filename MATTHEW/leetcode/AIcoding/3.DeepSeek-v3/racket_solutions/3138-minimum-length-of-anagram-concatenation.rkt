(define (min-anagram-length s)
  (let* ((n (string-length s))
         (freq (make-hash))
         (gcd-freq 0))
    (for ([c (in-string s)])
      (hash-update! freq c add1 0))
    (for ([(k v) (in-hash freq)])
    (set! gcd-freq (foldl gcd 0 (hash-values freq)))
    (if (> gcd-freq 0)
        (/ n gcd-freq)
        n)))