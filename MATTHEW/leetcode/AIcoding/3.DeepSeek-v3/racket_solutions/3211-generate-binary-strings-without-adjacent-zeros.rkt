(define (generate-binary-strings n)
  (define (helper current last-char)
    (if (= (string-length current) n)
        (list current)
        (if (equal? last-char #\0)
            (helper (string-append current "1") #\1)
            (append (helper (string-append current "0") #\0)
                    (helper (string-append current "1") #\1)))))
  (if (= n 0)
      '("")
      (append (helper "0" #\0) (helper "1" #\1))))