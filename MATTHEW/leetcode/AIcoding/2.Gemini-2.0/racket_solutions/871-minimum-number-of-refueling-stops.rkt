(define (min-refuel-stops target start-fuel stations)
  (let* ((n (length stations))
         (dp (make-vector (+ n 1) 0)))
    (vector-set! dp 0 start-fuel)
    (for/fold ((i 0))
              ((station stations))
      (let ((location (first station))
            (capacity (second station)))
        (for ((j (in-range i -1 -1)))
          (when (>= (vector-ref dp j) location)
            (vector-set! dp (+ j 1) (max (vector-ref dp (+ j 1)) (+ (vector-ref dp j) capacity))))
        (+ i 1)))
    (let loop ((i n))
      (cond
        ((< i 0) -1)
        ((>= (vector-ref dp i) target) i)
        (else (loop (- i 1)))))))