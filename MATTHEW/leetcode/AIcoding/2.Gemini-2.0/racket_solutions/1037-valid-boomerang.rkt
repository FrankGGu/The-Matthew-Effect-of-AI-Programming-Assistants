(define (is-boomerang points)
  (let ((x1 (car (list-ref points 0)))
        (y1 (cadr (list-ref points 0)))
        (x2 (car (list-ref points 1)))
        (y2 (cadr (list-ref points 1)))
        (x3 (car (list-ref points 2)))
        (y3 (cadr (list-ref points 2))))
    (and (not (and (= x1 x2) (= y1 y2)))
         (not (and (= x1 x3) (= y1 y3)))
         (not (and (= x2 x3) (= y2 y3)))
         (not (= (* (- x2 x1) (- y3 y1)) (* (- x3 x1) (- y2 y1)))))))