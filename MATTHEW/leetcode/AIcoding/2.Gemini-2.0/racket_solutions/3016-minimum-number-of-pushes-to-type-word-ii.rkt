(define (minimum-pushes s)
  (let* ((counts (make-vector 26 0))
         (n (string-length s)))
    (for ([i (in-range n)])
      (vector-set! counts (- (char->integer (string-ref s i)) (char->integer #\a)) (+ (vector-ref counts (- (char->integer (string-ref s i)) (char->integer #\a))) 1)))
    (let* ((sorted-counts (sort (vector->list counts) >))
           (total-pushes 0))
      (for ([i (in-range (length sorted-counts))])
        (set! total-pushes (+ total-pushes (* (add1 (quotient i 8)) (list-ref sorted-counts i)))))
      total-pushes)))