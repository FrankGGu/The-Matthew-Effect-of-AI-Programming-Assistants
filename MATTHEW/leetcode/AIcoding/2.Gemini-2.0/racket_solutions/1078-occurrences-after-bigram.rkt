(define (find-occurrences text first second)
  (let* ((words (string-split text " "))
         (n (length words))
         (result '()))
    (for ((i (in-range (- n 2))))
      (when (and (equal? (list-ref words i) first)
                 (equal? (list-ref words (+ i 1)) second))
        (set! result (append result (list (list-ref words (+ i 2)))))))
    result))