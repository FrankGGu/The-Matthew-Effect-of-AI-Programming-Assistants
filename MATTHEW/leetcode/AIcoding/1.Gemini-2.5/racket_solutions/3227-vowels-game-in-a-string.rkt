(define (is-vowel? char)
  (or (char=? char #\a)
      (char=? char #\e)
      (char=? char #\i)
      (char=? char #\o)
      (char=? char #\u)))

(define (vowels-game s)
  (let loop ((idx 0) (vowel-count 0))
    (if (= idx (string-length s))
        (odd? vowel-count)
        (loop (+ idx 1)
              (if (is-vowel? (string-ref s idx))
                  (+ vowel-count 1)
                  vowel-count)))))