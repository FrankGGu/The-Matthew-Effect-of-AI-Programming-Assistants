(define (convert-to-title columnNumber)
  (let loop ((n columnNumber) (chars '()))
    (if (zero? n)
        (string-reverse (list->string chars))
        (let* ((n-minus-1 (- n 1))
               (remainder (modulo n-minus-1 26))
               (char-code (+ (char->integer #\A) remainder))
               (char (integer->char char-code)))
          (loop (quotient n-minus-1 26) (cons char chars))))))