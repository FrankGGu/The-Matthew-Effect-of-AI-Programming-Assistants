(define (sum-four-divisors nums)
  (define (four-divisors-sum n)
    (define divisors '())
    (for ([i (in-range 1 (add1 (sqrt n)))])
      (when (= (modulo n i) 0)
        (set! divisors (cons i divisors))
        (when (not (= i (/ n i)))
          (set! divisors (cons (/ n i) divisors)))))
    (if (= (length divisors) 4)
        (apply + divisors)
        0))
  (apply + (map four-divisors-sum nums)))