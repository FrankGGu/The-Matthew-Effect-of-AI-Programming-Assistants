(define (checkSubarraySum nums k)
  (define n (length nums))
  (define sum-map (make-hash))
  (hash-set! sum-map 0 -1)
  (define sum 0)
  (for ([i (in-range n)])
    (set! sum (+ sum (list-ref nums i)))
    (define mod (if (= k 0) sum  (mod sum k)))
    (if (hash-has-key? sum-map mod)
        (if (> i (+ (hash-ref sum-map mod) 1))
            (return #t))
        (hash-set! sum-map mod i)))
  #f)