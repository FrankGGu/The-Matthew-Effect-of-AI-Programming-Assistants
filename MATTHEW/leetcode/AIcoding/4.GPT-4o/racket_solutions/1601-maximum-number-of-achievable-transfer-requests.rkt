(define (maximum-achievable-transfer-requests n requests)
  (define (dfs index count open)
    (if (>= index (length requests))
        (if (zero? open) count 0)
        (let* ((request (list-ref requests index))
               (from (car request))
               (to (cadr request)))
          (max (dfs (+ index 1) count open)
               (dfs (+ index 1) (+ count 1) (+ open 1))
               (dfs (+ index 1) count (- open 1))))))
  (dfs 0 0 0))

(define (maximumNumberOfAchievableTransferRequests n requests)
  (maximum-achievable-transfer-requests n requests))