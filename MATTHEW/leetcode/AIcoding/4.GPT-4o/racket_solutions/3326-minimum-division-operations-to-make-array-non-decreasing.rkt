(define (minOperations (nums '()))
  (define (helper nums)
    (define n (length nums))
    (define dp (make-vector n (expt 10 9)))
    (vector-set! dp 0 0)
    (for ([i (in-range n)])
      (for ([j (in-range i)])
        (when (<= (vector-ref nums j) (vector-ref nums i))
          (vector-set! dp i (min (vector-ref dp i) (vector-ref dp j)))))
      (vector-set! dp i (+ (vector-ref dp i) 1)))
    (apply min (vector->list dp)))
  (helper (vector->list (vector nums))))