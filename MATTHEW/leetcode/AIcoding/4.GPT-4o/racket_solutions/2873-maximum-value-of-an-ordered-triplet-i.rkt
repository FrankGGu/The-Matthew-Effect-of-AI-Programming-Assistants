(define (maximumValue nums)
  (define (valid? x)
    (and (string? x) (not (string-empty? x))))
  (define (to-number x)
    (if (valid? x)
        (string->number x)
        -1))
  (define max-value -1)
  (for-each (lambda (t)
              (let* ((a (to-number (list-ref t 0)))
                     (b (to-number (list-ref t 1)))
                     (c (to-number (list-ref t 2))))
                (set! max-value (max max-value (max a b c)))))
            nums)
  max-value)