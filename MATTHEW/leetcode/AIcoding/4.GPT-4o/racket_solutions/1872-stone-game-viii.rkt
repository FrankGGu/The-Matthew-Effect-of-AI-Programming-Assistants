(define (stoneGameVIII stones)
  (define n (length stones))
  (define prefix-sum (vector 0))
  (for ((i (in-range n)))
    (vector-set! prefix-sum (+ i 1) (+ (vector-ref prefix-sum i) (list-ref stones i))))
  (define max-score (vector-ref prefix-sum (sub1 n)))
  (define result max-score)
  (for ((i (in-range (sub1 n) 0 -1)))
    (set! result (max result (- max-score (vector-ref prefix-sum i)))))
  result)