(define (checkOverlap circle center radius)
  (define (square x) (* x x))
  (define (distance-squared x1 y1 x2 y2)
    (+ (square (- x1 x2)) (square (- y1 y2))))
  (let* ((circle-x (car center))
         (circle-y (cadr center))
         (dist-squared (distance-squared circle-x circle-y 0 0))
         (radius-squared (square radius))
         (circle-radius-squared (square (car circle)))
         (circle-center (cadr circle)))
    (<= dist-squared (+ radius-squared circle-radius-squared))))