(define (permuteUnique nums)
  (define (helper nums current result)
    (if (null? nums)
        (cons current result)
        (let ((seen '()))
          (for-each (lambda (num)
                      (when (not (memv num seen))
                        (set! seen (cons num seen))
                        (helper (filter (lambda (x) (not (= x num))) nums)
                                (cons num current)
                                result)))
                    nums))))
  (reverse (helper (sort nums <) '() '())))