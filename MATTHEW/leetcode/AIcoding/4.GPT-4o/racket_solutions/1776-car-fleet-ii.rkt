(define (carFleet II target position speed)
  (define n (length position))
  (define cars (map list position speed))
  (define sorted-cars (sort cars (lambda (a b) (> (car a) (car b)))))
  (define (time-to-reach car)
    (let ((pos (car car))
          (sp (cadr car)))
      (/ (- target pos) sp)))
  (define times (map time-to-reach sorted-cars))
  (define fleets '())
  (define current-max-time 0)
  (for ((i (in-range n)))
    (let ((t (list-ref times i)))
      (when (> t current-max-time)
        (set! current-max-time t)
        (set! fleets (cons 1 fleets)))))
  (length fleets))