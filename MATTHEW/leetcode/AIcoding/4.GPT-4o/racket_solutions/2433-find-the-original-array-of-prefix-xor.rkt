(define (find-original-array prefix)
  (define n (length prefix))
  (define original (make-vector (- n 1) 0))
  (for ([i (in-range 1 n)])
    (vector-set! original (- i 1) (xor (vector-ref prefix i) (vector-ref prefix (- i 1)))))
  (if (and (zero? (vector-ref prefix 0)) (= n 1))
      (list 0)
      (if (or (not (zero? (vector-ref prefix 0))) (not (eq? (vector-ref original 0) 0)))
          '()
          (vector->list original)))))