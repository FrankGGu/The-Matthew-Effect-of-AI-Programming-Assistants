(define (maxTaxiEarnings n rides)
  (define dp (make-vector (+ n 1) 0))
  (for-each (lambda (ride)
              (define start (first ride))
              (define end (second ride))
              (define tip (third ride))
              (define earnings (+ end tip))
              (for ([i (in-range end (+ 1))])
                (vector-set! dp i (max (vector-ref dp i) (+ (vector-ref dp start) earnings)))))
            rides)
  (vector-ref dp n))