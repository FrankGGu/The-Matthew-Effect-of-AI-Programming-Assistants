(define (can-see) 
  (define (visible-people heights)
    (define n (length heights))
    (define result (make-vector n 0))
    (define stack '())
    (for ([i (in-range n)])
      (begin
        (while (and (not (null? stack)) 
                    (> (vector-ref heights i) (car stack))))
          (vector-set! result (length stack) (add1 (vector-ref result (length stack))))
          (set! stack (cdr stack)))
        (set! stack (cons (vector-ref heights i) stack)))
      )
    result)
  (visible-people))