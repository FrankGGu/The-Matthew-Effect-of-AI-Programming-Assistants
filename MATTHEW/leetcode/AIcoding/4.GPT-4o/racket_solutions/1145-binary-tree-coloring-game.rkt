(define (binarTreeColoringGame (colors) (num)) 
  (define (dfs node)
    (if (null? node)
        (list 0 0)
        (let* ((left (dfs (car node)))
               (right (dfs (cadr node)))
               (left-count (car left))
               (right-count (car right))
               (total-count (+ left-count right-count 1)))
          (list total-count (+ (cadr left) (cadr right) (if (equal? (car colors) 'R) 1 0))))))
    )
  (define counts (dfs (list (list (car num) (cadr num)))))
  (> (cadr counts) (/ (car counts) 2)))