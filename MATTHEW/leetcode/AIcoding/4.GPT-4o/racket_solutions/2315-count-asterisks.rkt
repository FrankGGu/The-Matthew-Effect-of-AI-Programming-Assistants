(define (countAsterisks s)
  (define (count-in-between str)
    (if (string-contains? str "|")
        (let* ((parts (string-split str "|"))
               (left (car parts))
               (right (if (> (length parts) 1) (cadr parts) "")))
               (count-left (string-length (string-filter (lambda (c) (equal? c #\*))
                                                         left)))
               (count-right (count-in-between right)))
          (+ count-left count-right))
        (string-length (string-filter (lambda (c) (equal? c #\*)) str))))
  (count-in-between s))