(define (minOperations(nums))
  (define (check(x))
    (for/sum ([i (in-range (length nums))])
      (max 0 (- x (list-ref nums i)))))
  (define low 1)
  (define high (+ 1 (apply max nums)))
  (define result high)
  (define (bin-search)
    (if (<= low high)
        (let* ([mid (quotient (+ low high) 2)]
               [ops (check mid)])
          (if (= ops mid)
              (begin (set! result mid) (set! high (- mid 1)))
              (if (< ops mid)
                  (begin (set! result mid) (set! high (- mid 1)))
                  (set! low (+ mid 1)))))
          (bin-search)))
  (bin-search)
  result)

(define (minOperationsWrapper nums)
  (minOperations nums))