(define (make-good s)
  (define (helper lst)
    (if (null? lst)
        '()
        (let ((first (car lst))
              (rest (helper (cdr lst))))
          (if (and (not (null? rest))
                   (let ((second (car rest)))
                     (or (and (char<=? first second)
                             (= (char-downcase first) (char-downcase second))
                             (not (= first second)))
                         (and (char<=? second first)
                             (= (char-downcase second) (char-downcase first))
                             (not (= second first)))))
              )
              (cdr rest)
              (cons first rest)))))
  (list->string (helper (string->list s))))

(make-good "leEeetcode")