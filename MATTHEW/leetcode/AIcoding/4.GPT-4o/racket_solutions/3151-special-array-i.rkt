(define (specialArray nums)
  (define (check n)
    (let ((count 0))
      (for-each (lambda (x)
                  (when (>= x n)
                    (set! count (+ count 1))))
                nums)
      (= count n)))
  (define (find-special)
    (for ([n (in-range 0 (+ 1 (length nums)))])
      (when (check n)
        (return n))))
  (or (find-special) -1))