(define (merge-triplets triplets target)
  (define (can-form triplet)
    (and (<= (first triplet) (first target))
         (<= (second triplet) (second target))
         (<= (third triplet) (third target))))
  (define (is-target triplet)
    (and (= (first triplet) (first target))
         (= (second triplet) (second target))
         (= (third triplet) (third target))))
  (define filtered (filter can-form triplets))
  (define merged (foldl (lambda (a b)
                           (list (max (first a) (first b))
                                 (max (second a) (second b))
                                 (max (third a) (third b))))
                         '(0 0 0) filtered))
  (is-target merged))

(merge-triplets '((1 3 2) (2 2 1) (1 2 3)) '(2 3 3))