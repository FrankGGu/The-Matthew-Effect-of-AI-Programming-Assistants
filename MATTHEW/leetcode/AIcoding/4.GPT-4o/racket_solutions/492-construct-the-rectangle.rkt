(define (constructRectangle area)
  (define (find-factors n)
    (for/list ([i (in-range 1 (add1 (floor (sqrt n))))])
      (when (= (remainder n i) 0)
        (list i (/ n i)))))
  (define factors (find-factors area))
  (define max-width (apply max (map first factors)))
  (define length (/ area max-width))
  (list length max-width))