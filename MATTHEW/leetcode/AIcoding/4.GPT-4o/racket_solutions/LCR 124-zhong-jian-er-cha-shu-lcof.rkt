(define (find-root nodes)
  (define (find-child-parent pairs)
    (foldl (lambda (pair acc)
              (let ((child (second pair))
                    (parent (first pair)))
                (if (member parent acc)
                    (remove* child acc)
                    (cons child acc))))
            (map first pairs)
            pairs))
  (define pairs (map (lambda (node) (list (car node) (cadr node))) nodes))
  (car (find-child-parent pairs)))

(define (createBinaryTree descriptions)
  (if (null? descriptions)
      '()
      (let ((root (find-root descriptions)))
        (define tree (make-vector (length descriptions) #f))
        (for-each (lambda (desc)
                    (vector-set! tree (car desc) (list (cadr desc) (caddr desc))))
                  descriptions)
        tree)))

(define (search-root tree)
  (if (null? tree)
      '()
      (let ((left (search-root (vector-ref tree 0)))
            (right (search-root (vector-ref tree 1))))
        (cons (if (null? left) (vector-ref tree 0) left)
              (if (null? right) (vector-ref tree 1) right))))))

(define (binaryTreePaths descriptions)
  (define root (createBinaryTree descriptions))
  (search-root root))