(define (maximum-product nums)
  (let* ((n (length nums))
         (sorted-nums (sort nums >))
         (max1 (list-ref sorted-nums 0))
         (max2 (list-ref sorted-nums 1))
         (max3 (list-ref sorted-nums 2))
         (min1 (list-ref sorted-nums (- n 1)))
         (min2 (list-ref sorted-nums (- n 2))))
    (max (* max1 max2 max3) (* min1 min2 max1))))