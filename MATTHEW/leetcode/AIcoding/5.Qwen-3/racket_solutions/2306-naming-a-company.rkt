(define (distinct-words words)
  (define (count-unique s)
    (let ([set (set)])
      (for ([c s])
        (set-add! set c))
      (set-count set)))
  (define freq (make-hash))
  (for ([word words])
    (let ([cnt (count-unique word)])
      (hash-update! freq cnt add1 0)))
  (hash-values freq))

