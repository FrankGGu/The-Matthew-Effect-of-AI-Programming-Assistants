(define (number-of-similar-strings words)
  (define (count-unique s)
    (let ([set (set)])
      (for ([c s])
        (set-add! set c))
      (set-count set)))
  (define freq (make-hash))
  (for ([word words])
    (let ([cnt (count-unique word)])
      (hash-update! freq cnt add1 0)))
  (define res 0)
  (for ([v (hash-values freq)])
    (when (> v 1)
      (set! res (+ res (* v (- v 1))))))
  res)

