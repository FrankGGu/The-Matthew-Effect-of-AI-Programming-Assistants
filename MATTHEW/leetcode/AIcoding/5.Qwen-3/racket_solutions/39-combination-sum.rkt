(define (combination-sum candidates target)
  (define (backtrack start target path res)
    (cond ((= target 0) (set! res (append res (list (list-copy path)))))
          ((> target 0)
           (for ([i (in-range start (length candidates))])
             (when (<= (+ (list-ref path 0) (list-ref candidates i)) target)
               (set! path (append path (list (list-ref candidates i))))
               (backtrack (+ i 1) (- target (list-ref candidates i)) path res)
               (set! path (reverse (cdr (reverse path))))))))
  (define res '())
  (backtrack 0 target '() res)
  res)