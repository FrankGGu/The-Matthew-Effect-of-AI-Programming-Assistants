(define (count-increasing-quadruplets nums)
  (define n (length nums))
  (define count 0)
  (define dp (make-vector n 0))
  (for ([i (in-range n)])
    (for ([j (in-range i)])
      (when (< (list-ref nums j) (list-ref nums i))
        (vector-set! dp i (+ (vector-ref dp i) (vector-ref dp j) 1))))
    (set! count (+ count (vector-ref dp i))))
  count)