#lang racket

(define (find-targets nums)
  (define n (length nums))
  (define targets (make-vector n #f))
  (define count 0)
  (define (check i)
    (when (< i n)
      (when (not (vector-ref targets i))
        (vector-set! targets i #t)
        (set! count (+ count 1))
        (check (+ i 1)))))
  (for ([i (in-range n)])
    (when (not (vector-ref targets i))
      (check i)))
  count)

(define (destroy-sequential-targets nums)
  (define n (length nums))
  (define targets (make-vector n #f))
  (define count 0)
  (define (check i)
    (when (< i n)
      (when (not (vector-ref targets i))
        (vector-set! targets i #t)
        (set! count (+ count 1))
        (check (+ i 1)))))
  (for ([i (in-range n)])
    (when (not (vector-ref targets i))
      (check i)))
  count)