(define (number-of-ways s)
  (define mod 1000000007)
  (define n (string-length s))
  (define dp (make-vector n 0))
  (vector-set! dp 0 1)
  (for ([i (in-range 1 n)])
    (when (char=? #\0 (string-ref s i))
      (when (and (>= i 1) (not (char=? #\0 (string-ref s (sub1 i)))))
        (vector-set! dp i (vector-ref dp (sub1 i))))
      (when (and (>= i 2) (not (char=? #\0 (string-ref s (- i 2)))))
        (vector-set! dp i (vector-ref dp (- i 2)))))
    (when (not (char=? #\0 (string-ref s i)))
      (when (and (>= i 1) (not (char=? #\0 (string-ref s (sub1 i)))))
        (vector-set! dp i (modulo (+ (vector-ref dp i) (vector-ref dp (sub1 i))) mod)))
      (when (and (>= i 2) (not (char=? #\0 (string-ref s (- i 2))))
                 (<= (string->number (substring s (- i 2) i)) 26))
        (vector-set! dp i (modulo (+ (vector-ref dp i) (vector-ref dp (- i 2))) mod)))))
  (vector-ref dp (sub1 n)))