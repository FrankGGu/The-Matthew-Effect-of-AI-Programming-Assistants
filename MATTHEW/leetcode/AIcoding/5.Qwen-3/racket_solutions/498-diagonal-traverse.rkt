(define (find-diagonal-order matrix)
  (if (null? matrix)
      '()
      (let ((rows (length matrix))
            (cols (length (car matrix))))
        (let loop ((i 0) (j 0) (result '()))
          (if (or (>= i rows) (>= j cols))
              (reverse result)
              (let ((current (list-ref (list-ref matrix i) j)))
                (loop (+ i -1) (+ j 1) (cons current result))))))))