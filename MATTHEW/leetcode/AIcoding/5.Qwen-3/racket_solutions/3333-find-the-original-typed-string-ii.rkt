(define (decompress-rgb s)
  (define (parse-digit c)
    (- (char->integer c) (char->integer #\0)))
  (define (helper i res)
    (if (>= i (string-length s))
        res
        (let ((c (string-ref s i)))
          (if (char-numeric? c)
              (let ((count (parse-digit c)))
                (if (< i (+ 1 (string-length s)))
                    (let ((next-char (string-ref s (+ i 1))))
                      (if (char-numeric? next-char)
                          (let ((count2 (parse-digit next-char)))
                            (helper (+ i 2) (string-append res (make-string (* 10 count) next-char))))
                          (helper (+ i 1) (string-append res (make-string count c)))))
                (helper (+ i 1) (string-append res (make-string 1 c))))))
          (helper (+ i 1) (string-append res (make-string 1 c))))))
  (helper 0 ""))

(define (find-original-typed-string s)
  (define (helper i res)
    (if (>= i (string-length s))
        res
        (let ((c (string-ref s i)))
          (if (char-numeric? c)
              (let ((count (parse-digit c)))
                (if (< i (+ 1 (string-length s)))
                    (let ((next-char (string-ref s (+ i 1))))
                      (if (char-numeric? next-char)
                          (let ((count2 (parse-digit next-char)))
                            (helper (+ i 2) (string-append res (make-string (* 10 count) next-char))))
                          (helper (+ i 1) (string-append res (make-string count c)))))
                (helper (+ i 1) (string-append res (make-string 1 c))))))
          (helper (+ i 1) (string-append res (make-string 1 c))))))
  (helper 0 ""))

(define (main)
  (define input (read-line))
  (display (find-original-typed-string input)))

(main)