(define (min-number-for-digit-array nums1 nums2)
  (define (to-set lst)
    (list->set lst))
  (define s1 (to-set nums1))
  (define s2 (to-set nums2))
  (define common (set-intersection s1 s2))
  (if (set-empty? common)
      (let* ([min1 (apply min nums1)]
             [min2 (apply min nums2)])
        (if (< min1 min2)
            (string->number (format "~a~a" min1 min2))
            (string->number (format "~a~a" min2 min1))))
      (let ([min-common (apply min (set->list common))])
        (string->number (format "~a" min-common)))))