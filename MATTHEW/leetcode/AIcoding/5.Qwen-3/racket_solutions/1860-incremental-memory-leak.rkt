(define (leak-calculation x y)
  (let loop ((a x) (b y) (result '()))
    (if (and (= a 0) (= b 0))
        (reverse result)
        (let* ((max-val (max a b))
               (diff (- max-val a))
               (new-a (- a diff))
               (new-b (- b diff)))
          (loop new-a new-b (cons (list a b) result))))))