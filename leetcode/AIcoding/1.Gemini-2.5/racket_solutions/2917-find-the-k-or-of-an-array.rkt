(define (find-k-or nums k)
  (let loop-bits ((bit-pos 0) (k-or-result 0))
    (if (> bit-pos 30) ; Iterate up to bit 30 (for 32-bit integers, max value 10^9 fits in 30 bits)
        k-or-result
        (let ((current-bit-mask (arithmetic-shift 1 bit-pos)))
          (let loop-nums ((numbers nums) (bit-count 0))
            (if (null? numbers)
                (if (>= bit-count k)
                    (loop-bits (+ bit-pos 1) (bitwise-ior k-or-result current-bit-mask))
                    (loop-bits (+ bit-pos 1) k-or-result))
                (let ((num (car numbers)))
                  (if (not (zero? (bitwise-and num current-bit-mask)))
                      (loop-nums (cdr numbers) (+ bit-count 1))
                      (loop-nums (cdr numbers) bit-count)))))))))