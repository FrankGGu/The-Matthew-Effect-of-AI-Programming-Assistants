(define (binary-tree-paths root)
  (define (get-paths node current-path-string)
    (if (not node)
        '()
        (let* ((node-val (TreeNode-val node))
               (new-path-string (if (string=? current-path-string "")
                                    (number->string node-val)
                                    (string-append current-path-string "->" (number->string node-val)))))
          (if (and (not (TreeNode-left node)) (not (TreeNode-right node)))
              (list new-path-string)
              (append (get-paths (TreeNode-left node) new-path-string)
                      (get-paths (TreeNode-right node) new-path-string))))))
  (get-paths root ""))