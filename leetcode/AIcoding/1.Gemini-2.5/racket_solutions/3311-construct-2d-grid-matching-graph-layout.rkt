(define (construct-2d-grid original m n)
  (let ((len (length original)))
    (if (= len (* m n))
        (let loop ((k 0) (current-original original) (result '()))
          (if (= k m)
              (reverse result)
              (let-values (((row rest) (split-at current-original n)))
                (loop (+ k 1) rest (cons row result)))))
        '())))