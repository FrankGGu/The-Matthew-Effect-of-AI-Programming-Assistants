(define (is-rectangle-overlap rec1 rec2)
  (let* ((x1_1 (car rec1))
         (y1_1 (cadr rec1))
         (x2_1 (caddr rec1))
         (y2_1 (cadddr rec1))

         (x1_2 (car rec2))
         (y1_2 (cadr rec2))
         (x2_2 (caddr rec2))
         (y2_2 (cadddr rec2)))
    (and (> x2_1 x1_2)
         (> x2_2 x1_1)
         (> y2_1 y1_2)
         (> y2_2 y1_1))))