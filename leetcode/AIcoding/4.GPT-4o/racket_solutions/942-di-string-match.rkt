(define (di-string-match s)
  (let* ((n (string-length s))
         (result (make-vector (+ n 1) 0))
         (low 0)
         (high n))
    (for-each (lambda (char i)
                (if (char=? char #\I)
                    (vector-set! result i high)
                    (vector-set! result i low)
                    (set! low (+ low 1))
                    (set! high (- high 1))))
              (string->list s)
              (range 0 n))
    (vector->list result)))

(di-string-match "IDID")