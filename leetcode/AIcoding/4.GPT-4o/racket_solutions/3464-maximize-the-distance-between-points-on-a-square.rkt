(define (maxDistance(points))
  (define (distance p1 p2)
    (sqrt (+ (expt (- (car p1) (car p2)) 2)
             (expt (- (cadr p1) (cadr p2)) 2))))
  (define max-dist 0)
  (for ([i (in-range (length points))])
    (for ([j (in-range (length points))])
      (when (not (= i j))
        (set! max-dist (max max-dist (distance (list-ref points i) (list-ref points j)))))))
  max-dist)