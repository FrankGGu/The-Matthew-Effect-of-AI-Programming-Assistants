(define (rearrangeSticks n k)
  (define mod 1000000007)
  (define dp (make-vector (+ 1 n) 0))
  (vector-set! dp 0 1)
  (for ([i (in-range 1 (+ 1 n))])
    (for ([j (in-range (max 0 (- i k)) (+ 1 i))])
      (vector-set! dp i (mod (+ (vector-ref dp i) (vector-ref dp (- i 1))) mod)))
    (if (< k i)
      (vector-set! dp i (mod (- (vector-ref dp i) (vector-ref dp (- i k 1))) mod))))
  (vector-ref dp n))