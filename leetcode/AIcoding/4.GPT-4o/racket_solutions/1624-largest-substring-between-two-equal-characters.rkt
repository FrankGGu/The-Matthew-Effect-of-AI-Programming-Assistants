(define (maxLengthBetweenEqualCharacters s)
  (define indices (make-hash))
  (define max-length -1)
  (for ([i (in-range (string-length s))])
    (define char (string-ref s i))
    (if (hash-has-key? indices char)
        (set! max-length (max max-length (- i (hash-ref indices char 0 0))))
        (hash-set! indices char i)))
  max-length)