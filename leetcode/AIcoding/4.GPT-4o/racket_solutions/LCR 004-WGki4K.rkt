(define (singleNumber nums)
  (define (count-bits nums)
    (foldl (lambda (num acc)
              (for/list ([i (in-range 32)])
                (+ (if (bitwise-and num (ash 1 i)) 1 0) 
                   (list-ref acc i))))
            (make-vector 32 0)
            nums))
  (define bits (count-bits nums))
  (define result 0)
  (for ([i (in-range 32)])
    (when (= (vector-ref bits i) 1)
      (set! result (bitwise-ior result (ash 1 i)))))
  result)