(define (relativeSortArray arr1 arr2)
  (define freq (make-hash))
  (for-each (lambda (x) (hash-set! freq x (+ (hash-ref freq x 0) 1))) arr1)
  (define sorted-arr (sort arr1 <))
  (define result '())
  (for-each (lambda (x)
              (let ((count (hash-ref freq x 0)))
                (set! result (append result (make-list count x)))))
            arr2)
  (for-each (lambda (x) (set! result (append result (list x))))
            (filter (lambda (x) (not (hash-has? freq x))) sorted-arr))
  result)