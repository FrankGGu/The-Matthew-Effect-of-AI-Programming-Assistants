(define (odd-string-difference words)
  (define (calculate-difference word)
    (map (lambda (i) (- (char->integer (string-ref word (+ i 1))) 
                         (char->integer (string-ref word i))))
         (range 0 (- (string-length word) 1)))
    )
  (define (string->hash word)
    (let ((diffs (calculate-difference word)))
      (string-append (map number->string diffs))))
  (define hashes (map string->hash words))
  (define unique-hashes (remove-duplicates hashes))
  (if (= (length unique-hashes) 1)
      (car (filter (lambda (w) (equal? (string->hash w) (car unique-hashes))) words))
      ""))

(define (oddStringDifference words)
  (odd-string-difference words))