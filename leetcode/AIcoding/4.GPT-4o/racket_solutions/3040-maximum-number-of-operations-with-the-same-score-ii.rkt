(define (maximumOperations(nums))
  (define (helper nums target)
    (define n (length nums))
    (define dp (make-vector (+ target 1) 0))
    (vector-set! dp 0 1)
    (for-each (lambda (num)
                (for/fold ([j target]) ([j target] (in-range target -1 -1))
                  (if (>= j num)
                      (+ (vector-ref dp (- j num)) (vector-ref dp j))
                      (vector-ref dp j))))
              nums)
    (vector-ref dp target))

  (define max-score (apply max nums))
  (define target 0)
  (for-each (lambda (num) (set! target (+ target (- max-score num)))) nums)
  (helper nums target))

(maximumOperations '(2 2 1 1 5))