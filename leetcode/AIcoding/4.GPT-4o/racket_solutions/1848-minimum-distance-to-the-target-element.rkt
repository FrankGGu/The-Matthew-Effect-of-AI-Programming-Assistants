(define (getMinDistance nums target start)
  (define (find-index lst value)
    (for/fold ([index -1]) ([i (in-index lst)])
      (if (equal? (second i) value)
          (if (= index -1) (first i) index)
          index)))
  (define target-index (find-index nums target))
  (define left (for/fold ([min-dist (infinity)]) ([i (in-index nums)])
    (if (equal? (second i) target)
        (min min-dist (abs (- (first i) start)))
        min-dist)))
  (if (= target-index -1) -1 left))