(define (gcdOfStrings str1 str2)
  (define (gcd a b)
    (if (empty? b) a (gcd b (remainder a b))))
  (define (check-divisible str div)
    (and (= (remainder (string-length str) (string-length div)) 0)
         (equal? (string-append (make-string (/ (string-length str) (string-length div)) div)) str)))
  (if (equal? (string-append str1 str2) (string-append str2 str1))
      (let ((gcd-length (gcd (string-length str1) (string-length str2))))
        (substring str1 0 gcd-length))
      ""))