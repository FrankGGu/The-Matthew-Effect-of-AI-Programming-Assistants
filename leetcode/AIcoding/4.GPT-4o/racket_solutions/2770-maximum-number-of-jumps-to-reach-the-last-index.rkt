(define (maxJumps nums)
  (define n (length nums))
  (define dp (make-vector n -1))
  (define (helper i)
    (if (>= i n) 0)
    (if (vector-ref dp i) (vector-ref dp i))
    (define max-jumps 0)
    (for ([j (range (+ i 1) n)])
      (when (<= j (+ i (vector-ref nums i)))
        (set! max-jumps (max max-jumps (helper j)))))
    (set! (vector-ref dp i) (+ 1 max-jumps))
    (vector-ref dp i))
  (helper 0))

(maxJumps '(2 5 0 0))