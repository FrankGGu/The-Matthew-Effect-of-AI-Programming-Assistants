(define (findMaxPositiveInteger nums)
  (define set (make-hash))
  (for-each (lambda (num) (hash-set! set num #t)) nums)
  (define max-positive -1)
  (for-each (lambda (num)
              (when (and (> num 0) (hash-has-key? set (- num)))
                (set! max-positive (max max-positive num))))
            nums)
  max-positive)