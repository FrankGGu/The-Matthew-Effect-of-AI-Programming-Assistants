(define (numRabbits answers)
  (define counts (make-hash))
  (for-each (lambda (x)
              (hash-update! counts x (lambda (v) (+ v 1)) 0))
            answers)
  (define total 0)
  (for-each (lambda (k v)
              (define group-size (+ k 1))
              (set! total (+ total (ceiling (/ v group-size)))))
            (hash->list counts))
  total)