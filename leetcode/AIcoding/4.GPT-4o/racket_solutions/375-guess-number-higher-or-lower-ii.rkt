(define (getMoneyAmount n)
  (define dp (make-vector (+ n 1) (make-vector (+ n 1) 0)))
  (for ([i (in-range 1 (+ n 1))])
    (vector-set! dp i i 0))
  (for ([len (in-range 2 (+ n 1))])
    (for ([i (in-range 1 (+ (- n len) 2))])
      (define j (+ i len))
      (define min-cost +inf.0)
      (for ([k (in-range i (+ j 1))])
        (define cost (+ k (max (vector-ref dp i (- k 1)) (vector-ref dp (+ k 1) j))))
        (set! min-cost (min min-cost cost)))
      (vector-set! dp i j min-cost)))
  (vector-ref dp 1 n))