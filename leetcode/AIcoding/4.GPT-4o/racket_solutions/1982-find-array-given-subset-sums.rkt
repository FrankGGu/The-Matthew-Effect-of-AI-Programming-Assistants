(define (find-array sums)
  (define n (length sums))
  (define dp (make-vector (add1 (apply max sums)) #f))
  (vector-set! dp 0 '())
  (for/fold ([result '()]) ([sum sums])
    (for ([i (in-range (length dp))])
      (when (and (not (vector-ref dp i))
                 (not (>= i (length dp)))
                 (not (vector-ref dp (- i sum))))
        (vector-set! dp i (cons sum (vector-ref dp (- i sum))))
        (set! result (cons sum result)))))
  (if (vector-ref dp 0)
      (remove (lambda (x) (null? x)) (vector->list (vector-ref dp 0)))
      '()))