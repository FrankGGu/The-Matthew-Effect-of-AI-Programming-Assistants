(define (kth-palindrome intLength query)
  (define half-length (quotient (+ intLength 1) 2))
  (define start (expt 10 (- half-length 1)))
  (define end (- (expt 10 half-length) 1))

  (define (generate-palindrome n intLength)
    (define str (number->string n))
    (define reversed-str (list->string (reverse (string->list str))))
    (if (even? intLength)
        (string->number (string-append str reversed-str))
        (string->number (string-append str (substring reversed-str 1))))
    )

  (map (lambda (q)
         (let ((num (+ start (- q 1))))
           (if (<= start num end)
               (generate-palindrome num intLength)
               -1)))
       query))