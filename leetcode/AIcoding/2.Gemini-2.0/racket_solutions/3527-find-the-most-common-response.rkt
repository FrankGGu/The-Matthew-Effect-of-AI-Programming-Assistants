(define (most-common-response responses)
  (let ((counts (make-hash)))
    (for-each (lambda (response)
                (hash-update! counts response add1 0))
              responses)
    (let loop ((responses (hash-keys counts))
               (max-response #f)
               (max-count 0))
      (if (null? responses)
          max-response
          (let ((response (car responses))
                (count (hash-ref counts response)))
            (if (> count max-count)
                (loop (cdr responses) response count)
                (loop (cdr responses) max-response max-count)))))))