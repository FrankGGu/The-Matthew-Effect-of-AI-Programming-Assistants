(define (kth-distinct arr k)
  (let* ((counts (make-hash))
         (_ (for-each (lambda (s)
                        (hash-update! counts s (lambda (v) (+ v 1)) 1))
                      arr))
         (distinct-strings (filter (lambda (s) (= (hash-ref counts s) 1)) arr)))
    (if (and (>= k 1) (<= k (length distinct-strings)))
        (list-ref distinct-strings (- k 1))
        "")))