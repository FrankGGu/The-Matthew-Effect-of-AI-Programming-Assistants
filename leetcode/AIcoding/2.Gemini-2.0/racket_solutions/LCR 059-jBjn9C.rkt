(define (KthLargest k nums)
  (define heap (make-heap <))
  (for-each (lambda (x)
              (heap-add! heap x)
              (when (> (heap-count heap) k)
                (heap-remove-min! heap)))
            nums)
  (lambda (val)
    (heap-add! heap val)
    (when (> (heap-count heap) k)
      (heap-remove-min! heap))
    (heap-top heap)))