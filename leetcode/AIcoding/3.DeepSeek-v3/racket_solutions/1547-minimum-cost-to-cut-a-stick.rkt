(define/contract (min-cost n cuts)
  (-> exact-integer? (listof exact-integer?) exact-integer?)
  (let* ([cuts (sort (cons 0 (cons n cuts)) <])
    (let ([m (length cuts)])
      (let ([dp (make-vector m (make-vector m 0))])
        (for ([l (in-range 2 m)])
          (for ([i (in-range 0 (- m l))])
            (let ([j (+ i l)])
              (vector-set! (vector-ref dp i) j +inf.0)
              (for ([k (in-range (+ i 1) j)])
                (let ([cost (+ (vector-ref (vector-ref dp i) k)
                                (vector-ref (vector-ref dp k) j)
                                (- (list-ref cuts j) (list-ref cuts i)))])
                  (when (< cost (vector-ref (vector-ref dp i) j))
                    (vector-set! (vector-ref dp i) j cost))))))))
        (vector-ref (vector-ref dp 0) (- m 1)))))