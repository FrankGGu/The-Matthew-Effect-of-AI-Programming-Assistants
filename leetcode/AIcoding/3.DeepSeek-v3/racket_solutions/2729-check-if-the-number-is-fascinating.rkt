(define/contract (is-fascinating n)
  (-> exact-integer? boolean?)
  (let* ((n2 (* n 2))
         (n3 (* n 3))
         (s (string-append (number->string n) (number->string n2) (number->string n3)))
         (digits (string->list s)))
    (and (= (length digits) 9)
         (not (member #\0 digits))
         (= (length (remove-duplicates digits)) 9))))