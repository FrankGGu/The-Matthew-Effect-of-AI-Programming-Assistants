(define (freq-alphabets s)
  (let loop ([i 0] [result '()])
    (if (>= i (string-length s))
        (list->string (reverse result))
        (let ([c (if (and (< (+ i 2) (string-length s)) 
                      (string-ref s (+ i 2))
                      #\0)])
          (if (char=? c #\#)
              (let ([num (string->number (substring s i (+ i 2)))])
                (loop (+ i 3) (cons (integer->char (+ num 96)) result)))
              (let ([num (string->number (substring s i (+ i 1)))])
                (loop (+ i 1) (cons (integer->char (+ num 96)) result))))))))