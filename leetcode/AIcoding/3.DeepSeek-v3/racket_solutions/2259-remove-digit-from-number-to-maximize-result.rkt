(define/contract (remove-digit number digit)
  (-> string? string? string?)
  (define max-result "")
  (for ([i (in-range (string-length number))])
    (when (char=? (string-ref number i) (string-ref digit 0))
      (define candidate (string-append (substring number 0 i) (substring number (add1 i))))
      (when (or (string=? max-result "") (string>? candidate max-result))
        (set! max-result candidate))))
  max-result)