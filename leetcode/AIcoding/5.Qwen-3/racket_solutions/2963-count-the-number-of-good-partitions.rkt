(define (count-number-of-good-partitions s)
  (define n (string-length s))
  (define last (make-hash))
  (for ([i (in-range n)])
    (hash-set! last (string-ref s i) i))
  (define res 0)
  (define j 0)
  (define i 0)
  (for ([i (in-range n)])
    (set! j (max j (hash-ref last (string-ref s i))))
    (when (= i j)
      (set! res (+ res 1))))
  res)