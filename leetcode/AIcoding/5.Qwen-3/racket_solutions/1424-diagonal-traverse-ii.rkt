(define (find-diagonal-order nums)
  (define n (length nums))
  (define result '())
  (define (helper i j)
    (when (and (<= 0 i n) (<= 0 j (length (list-ref nums i))))
      (set! result (cons (list-ref (list-ref nums i) j) result))
      (helper (sub1 i) (add1 j))
      (helper (add1 i) (sub1 j))))
  (for ([i (in-range n)])
    (helper i 0))
  (reverse result))