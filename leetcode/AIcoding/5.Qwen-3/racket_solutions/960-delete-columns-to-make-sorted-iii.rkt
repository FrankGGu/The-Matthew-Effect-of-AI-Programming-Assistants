(define (delete-columns-to-make-sorted-iii strs)
  (define n (length strs))
  (define m (if (> n 0) (string-length (car strs)) 0))
  (define dp (make-vector m 1))
  (for ([j (in-range m)])
    (for ([i (in-range j)])
      (when (and (<= i j) (andmap (lambda (k) (char<=? (string-ref (list-ref strs k) i) (string-ref (list-ref strs k) j))) (range n)))
        (vector-set! dp j (max (vector-ref dp j) (+ (vector-ref dp i) 1))))))
  (apply max (vector->list dp)))