(define (min-operations nums)
  (define (sort-list lst)
    (if (null? lst)
        '()
        (let ((pivot (car lst)))
          (append (sort-list (filter (lambda (x) (< x pivot)) lst))
                  (list pivot)
                  (sort-list (filter (lambda (x) (>= x pivot)) (cdr lst)))))))
  (define sorted (sort-list nums))
  (define (helper lst prev count)
    (cond ((null? lst) count)
          ((= (car lst) prev) (helper (cdr lst) prev (+ count 1)))
          (else (helper (cdr lst) (car lst) count))))
  (helper sorted -inf.0 0))