(define (num-possible-permutations s)
  (define mod 1000000007)
  (define n (string-length s))
  (define dp (make-vector (+ n 1) (make-vector (+ n 1) 0)))
  (vector-set! dp 0 0 1)
  (for ([i (in-range 1 (+ n 1))])
    (for ([j (in-range 0 i)])
      (if (= (string-ref s (- i 1)) #\D)
          (begin
            (for ([k (in-range 0 j)])
              (vector-set! dp i j (modulo (+ (vector-ref dp i j) (vector-ref dp (- i 1) k)) mod))))
          (begin
            (for ([k (in-range j i)])
              (vector-set! dp i j (modulo (+ (vector-ref dp i j) (vector-ref dp (- i 1) k)) mod)))))))
  (apply + (vector->list (vector-take dp n))) mod)