#lang racket

(define (make-queue) (cons '() '()))
(define (enqueue q x) (set-cdr! q (cons x (cdr q))) x)
(define (dequeue q) (let ((x (car q))) (set-car! q (cdr q)) x))
(define (queue-empty? q) (null? (car q)))

(define (task-scheduler root)
  (define (level-order-traversal root)
    (define q (make-queue))
    (enqueue q root)
    (let loop ()
      (when (not (queue-empty? q))
        (let ((size (length (cdr q))))
          (for ([i (in-range size)])
            (let ((node (dequeue q)))
              (when (not (null? (node-left node)))
                (enqueue q (node-left node)))
              (when (not (null? (node-right node)))
                (enqueue q (node-right node)))))
        (loop))))
  (define (node-left node) (vector-ref node 0))
  (define (node-right node) (vector-ref node 1))
  (level-order-traversal root)
  (void))