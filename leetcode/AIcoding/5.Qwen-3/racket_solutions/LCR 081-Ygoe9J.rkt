(define (combination-sum candidates target)
  (define (backtrack start target path result)
    (cond ((= target 0) (set! result (cons (reverse path) result)))
          ((> target 0)
           (for ([i (in-range start (length candidates))])
             (when (<= (+ (list-ref candidates i) target) target)
               (backtrack (+ i 1) (- target (list-ref candidates i)) (cons (list-ref candidates i) path) result)))))
    result)
  (backtrack 0 target '() '()))