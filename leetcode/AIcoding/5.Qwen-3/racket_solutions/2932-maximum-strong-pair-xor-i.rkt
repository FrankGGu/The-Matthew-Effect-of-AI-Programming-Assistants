(define (maximum-strong-pair-xor nums)
  (define (strong-pair? a b)
    (<= (abs (- a b)) (min a b)))
  (define (max-xor a b)
    (bitwise-xor a b))
  (define (helper i j max-val)
    (if (>= i (length nums))
        max-val
        (if (>= j (length nums))
            (helper (+ i 1) 0 max-val)
            (if (strong-pair? (list-ref nums i) (list-ref nums j))
                (helper i (+ j 1) (max max-val (max-xor (list-ref nums i) (list-ref nums j))))
                (helper i (+ j 1) max-val))))
  )
  (helper 0 0 0))