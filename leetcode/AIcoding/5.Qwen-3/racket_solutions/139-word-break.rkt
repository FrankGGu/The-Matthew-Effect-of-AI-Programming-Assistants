#lang racket

(define (word-break s word-dict)
  (define memo (make-hash))
  (define (helper remaining)
    (if (hash-has-key? memo remaining)
        (hash-ref memo remaining)
        (let ((result #f))
          (when (equal? remaining "")
            (set! result #t))
          (for-each (lambda (word)
                      (when (and (<= (string-length word) (string-length remaining))
                                 (equal? (substring remaining 0 (string-length word)) word))
                        (when (helper (substring remaining (string-length word)))
                          (set! result #t))))
                    word-dict)
          (hash-set! memo remaining result)
          result)))
  (helper s))